{
    "version": "0.1",
    "prefixes": {
        "foaf": "http://xmlns.com/foaf/0.1/",
        "besluit": "http://data.vlaanderen.be/ns/besluit#",
        "dct": "http://purl.org/dc/terms/",
        "skos": "http://www.w3.org/2004/02/skos/core#"
    },
    "resources": {
        "users": {
            "name": "user",
            "class": "foaf:Person",
            "attributes": {
                "first-name": {
                    "type": "string",
                    "predicate": "foaf:firstName"
                },
                "family-name": {
                    "type": "string",
                    "predicate": "foaf:familyName"
                }
            },
            "relationships": {
                "accounts": {
                    "predicate": "foaf:account",
                    "target": "account",
                    "cardinality": "many"
                },
                "groups": {
                    "predicate": "foaf:member",
                    "target": "group",
                    "cardinality": "many"
                }
            },
            "new-resource-base": "http://data.lblod.info/id/gebruiker/"
        },
        "accounts": {
            "name": "account",
            "class": "foaf:OnlineAccount",
            "attributes": {
                "provider": {
                    "type": "string",
                    "predicate": "foaf:accountServiceHomepage"
                },
                "identifier": {
                    "type": "string",
                    "predicate": "dct:identifier"
                }
            },
            "relationships": {
                "user": {
                    "predicate": "foaf:account",
                    "target": "user",
                    "cardinality": "one",
                    "inverse": true
                }
            },
            "new-resource-base": "http://data.lblod.info/id/account/"
        },
        "groups": {
            "name": "group",
            "class": "besluit:Bestuurseenheid",
            "attributes": {
                "name": {
                    "type": "string",
                    "predicate": "skos:prefLabel"
                }
            },
            "relationships": {},
            "new-resource-base": "http://data.lblod.info/id/bestuurseenheden/"
        }
    }
}